<div class="container">
    <div class="row">
    <% for(i=0;i<oneProduct.length;i++) { %>
    <div class="col-lg-3 col-md-6 col-sm-12 pb-1">
        <div class="card product-item border-0 mb-4">
            <div class="card-header product-img position-relative overflow-hidden bg-transparent border p-0">
                <img class="img-fluid w-100" src="/images/<%=oneProduct[i].orderproducts[0].picture %>" style="width: 20rem;height:20rem;" alt="image not found">
            </div>
            <div class="card-body border-left border-right text-center p-0 pt-4 pb-3">
                <h6 class="text-truncate mb-3"><%= oneProduct[i].orderproducts[0].productName %></h6>
                <div class="d-flex justify-content-center">
                    <h6>₹<%= oneProduct[i].orderproducts[0].sellingPrice %></h6><h6 class="text-muted ml-2"><del>₹<%= oneProduct[i].orderproducts[0].actualPrice %></del></h6>
                </div>
            </div>
           
        </div>
    </div>

    <% } %>
</div>
</div>